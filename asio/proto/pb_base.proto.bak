// https://developers.google.com/protocol-buffers/docs/tutorials

// [START declaration]
syntax = "proto3";
package PbBase;

// [START messages]

message SubscribeReq
{
  int32 type = 1; // 0:删除，1:增加
  repeated string typeNameList = 2;
}

message SubscribeRsp
{
  int32 errorcode = 1;
}

message ServerInfoPub
{
  string hello = 1;
}

message HelloReq {
  string name = 1;
  int32 id = 2;
  bytes address = 3;
}

message HelloRsp {
  bytes hello = 1;
}

message EchoReq
{
  string content = 1;
}

message EchoRsp
{
  int32 errorcode = 1;
  string content = 2;
}

message HeartbeatReq
{
  int32 cpu = 1;	   // 百分比
  int32 memory = 2;	   // 百分比
}

message HeartbeatRsp
{
  int64 serverTime = 1;    // 服务器时间
}